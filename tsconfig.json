{
  "compilerOptions": {
    "target": "esnext",
    "allowJs": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": true,
    "jsx": "preserve",
    "typeRoots": [
      "./src/types", "./node_modules/@types"]
  },
  "include": [
    "src"
  ]
}
